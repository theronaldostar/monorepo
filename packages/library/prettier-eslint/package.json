	},
	"devDependencies": {
		"@eslint/js": "^9.21.0",
		"@typescript-eslint/eslint-plugin": "^8.25.0",
		"@typescript-eslint/parser": "^8.25.0",
		"eslint-plugin-eslint-config": "^2.0.0",
		"eslint-plugin-react-hooks": "^5.1.0",
		"eslint-plugin-react-refresh": "^0.4.19",
		"globals": "^16.0.0",
		"prettier-eslint": "^16.3.0",
